{
  "name": "turbomerch",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push --accept-data-loss && next build",
    "db:push": "prisma db push",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "bootstrap:status": "tsx scripts/bootstrap-marketplace.ts --status",
    "bootstrap:quick": "tsx scripts/bootstrap-marketplace.ts --quick",
    "bootstrap:full": "tsx scripts/bootstrap-marketplace.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.937.0",
    "@clerk/nextjs": "^6.8.2",
    "@google/genai": "latest",
    "@prisma/client": "^6.2.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "next": "^16.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^17.5.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.0.7",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.35",
    "prisma": "^6.2.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vitest": "^4.0.14"
  }
}
